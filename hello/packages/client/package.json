{
  "name": "angel_client",
  "version": "1.0.0-dev",
  "description": "Client library for the Angel framework.",
  "main": "build/angel_client.js",
  "jsnext:main": "lib/angel_client.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "npm run dartdevc && babel -o build/angel_client.js lib/angel_client.js",
